{
  "name": "@medishen/qiks",
  "version": "1.0.0",
  "description": "small cache system",
  "main": "tsconfig.js",
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
  },
  "scripts": {
    "test": "npm run test:unit & npm run test:e2e & npm run test:performance",
    "test:unit": "mocha --require ts-node/register tests/unit/**/*.spec.ts",
    "test:integration": "mocha --require ts-node/register tests/integration/**/*.spec.ts",
    "test:e2e": "mocha --require ts-node/register tests/e2e/*.spec.ts",
    "test:performance": "mocha --require ts-node/register tests/performance/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/medishen/qiks.git"
  },
  "keywords": [
    "cache",
    "cache-system",
    "lru"
  ],
  "engines": {
    "node": ">= 20"
  },
  "author": "Mahdi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/medishen/qiks/issues"
  },
  "homepage": "https://github.com/medishen/qiks#readme",
  "dependencies": {
    "@types/chai": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "chai": "^4.3.7",
    "mocha": "^11.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
